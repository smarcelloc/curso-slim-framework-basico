{
  "config": {
    "vendor-dir": "vendor",
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^7.3",
    "doctrine/annotations": "^1.13",
    "doctrine/orm": "^2.12",
    "respect/validation": "^2.2",
    "slim/slim": "3.*",
    "symfony/cache": "^5.4",
    "tuupola/slim-basic-auth": "^3.3",
    "vlucas/phpdotenv": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/config.php",
      "app/helpers.php"
    ]
  },
  "require-dev": {
    "symfony/console": "^5.4"
  }
}
